{
    "extends": "./tsconfig",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "compilerOptions": {
        "module": "commonjs",
        "outDir": "./build",
        "sourceMap": true,
        "noEmit": false
    },
    "include": [
        "src/server",
        "src/structs"
    ],
    "exclude": [
        "src/app"
    ]
  }